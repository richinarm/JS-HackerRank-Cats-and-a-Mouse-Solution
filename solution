function catAndMouse(x, y, z) {
  let catA = x;
  let catB = y;
  let mouse = z;

  let aResult = mouse - catA;
  let bResult = mouse - catB;


  if (aResult < 0) {
    aResult = catA - mouse;
  }

  if (bResult < 0) {
    bResult = catB - mouse;
  }

  if (aResult === bResult) {
    return "Mouse C"
  }

  if (aResult > bResult) {

    return "Cat B"
  }

  if (bResult > aResult) {
    return "Cat A"
  }
}

catAndMouse(1, 3, 2)
